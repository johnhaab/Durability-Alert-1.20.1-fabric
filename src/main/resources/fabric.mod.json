{
	"schemaVersion": 1,
	"id": "durability-alert-mod",
	"version": "${version}",
	"name": "Durability Alert",
	"description": "This is a mod that will give you Durability Alert's when your stuff gets low!",
	"authors": [
		"keziumo"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/durability-alert-mod/icon.png",
